const axios = require('axios');
// Fungsi untuk mengambil informasi jumlah gold
async function getGoldCount(showroomId) { try { const response = await 
    axios.get(`https://www.showroom-live.com/api/live/room_status?room_id=${showroomId}`); const 
    goldCount = response.data.room_status.general_user_data.gold; return goldCount;
  } catch (error) {
    console.error('Gagal mengambil informasi jumlah gold:', error); throw error;
  }
}
// Contoh penggunaan
const showroomId = '12345678'; // Ganti dengan ID showroom yang valid
